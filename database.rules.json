{
  "rules": {
    ".read": "auth.admin == true",
    ".write": "auth.admin == true",
      
    "users": {
      "$uid": {
        ".read": "auth.token.firebase.identities['facebook.com'][0] == $uid",
        ".write": "auth.token.firebase.identities['facebook.com'][0] == $uid"
      }
      
    }
  }
}